<core:View
	controllerName="demo.view.MaintainVehicle"
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Page 
		title="{i18n>MaintainVehicleView}"
		showNavButton="true"
		navButtonPress="handleNavButtonPress">
		<content>
			<List items="{/d/results}" >
			<StandardListItem 
				visible="true"
				unread="false"
				selected="false"
				description="{/lat} {/lng}"
				icon="sap-icon://bus-public-transport"
				iconInset="true"
				iconDensityAware="true"
				infoState="None"
				adaptTitleSize="true"
				titleTextDirection="Inherit"
				infoTextDirection="Inherit"
				tap=""
				detailTap=""
				detailPress=""
				type="Active"
				press="handleNavButtonPressTo"
				title="{Trip} {lat} {lng}" />
			</List>
		
		</content>
	</Page>
</core:View>